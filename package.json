{
  "name": "buckley-sitzman-com",
  "version": "1.0.0",
  "description": "Buckley Sitzman Home-Page",
  "main": "index.js",
  "repository": "https://github.com/Buckley-Sitzman-LLP/buckley-sitzman-com",
  "author": "Andrew Ek <andrewek@gmail.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "eleventy --serve & postcss ./src/assets/styles/tailwind.css --o _site/style.css --watch",
    "build": "NODE_ENV=production ELEVENTY_ENV=production eleventy && postcss ./src/assets/styles/tailwind.css --o _site/style.css",
    "a11y": "pa11y --reporter json > a11y_reports/pa11y-report.json",
    "axe": "axe --save a11y_reports/axe-report.json",
    "lighthouse": "lighthouse --only-categories=accessibility --chrome-flags=\"--headless\" --output json --output-path=a11y_reports/lighthouse-report",
    "audit:a11y": "node scripts/audit-accessibility.js"
  },
  "devDependencies": {
    "@11ty/eleventy": "3.1.2",
    "@11ty/eleventy-img": "^6.0.4",
    "@11ty/eleventy-plugin-syntaxhighlight": "^5.0.2",
    "@axe-core/cli": "^4.11.0",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.19",
    "autoprefixer": "^10.4.21",
    "baseline-browser-mapping": "^2.9.18",
    "html-minifier": "^4.0.0",
    "lighthouse": "^13.0.1",
    "pa11y": "^9.0.1",
    "postcss": "^8.5.6",
    "postcss-cli": "^11.0.1",
    "tailwindcss": "^3.4.18",
    "xml2js": "^0.6.2"
  }
}
